                                                                                                    <td>{{item.level}}</td>
                                                                                                    <td>
                                                                                                        <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#myPayout">Pay Out</button>
                                                                                                    </td>
                                                                                                </tr>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-sm-12 col-sm-offset-0" id="obj5733e78df37b4a276f98e9c0">
                                                                                <div class="bool-container panel panel-primary">
                                                                                    <div class="panel-heading" id="obj5733e78df37b4a276f98e9c1">
                                                                                        <div class="bool-container panel-title">
                                                                                            <h3>New Users </h3>
                                                                                            <center>
                                                                                                <div class="pull-right">
                                                                                                    <button type="button" class="btn btn-success btn-sm" data-toggle="modal" data-target="#myPayout3" ng-click="initPayout3()">Initialise Payout</button>
                                                                                                </div>
                                                                                            </center>
                                                                                            <h3> <span class="label label-default">Total: {{'E' + totamtL3 + '.00'}}</span></h3>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col-sm-12 col-sm-offset-0" id="obj5733e78df37b4a276f98e9c2">
                                                                                        <div class="bool-container panel-body">
                                                                                            <table class="table">
                                                                                                <tr>
                                                                                                    <th>Userid</th>
                                                                                                    <th>Username</th>
                                                                                                    <th>Contact</th>
                                                                                                    <th>Account #</th>
                                                                                                    <th>Account Holder</th>
                                                                                                </tr>
                                                                                                <tr ng-repeat="item in level3Users track by $index">
                                                                                                    <td>{{item._id}}</td>
                                                                                                    <td>{{item.uname}}</td>
                                                                                                    <td>{{item.contact}}</td>
                                                                                                    <td>{{item.acc}}</td>
                                                                                                    <td>{{item.accholder}}</td>
                                                                                                </tr>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <!-- Modal -->
                                                                            <div id="myPayout3" class="modal fade" role="dialog">
                                                                                <div class="modal-dialog">
                                                                                    <!-- Modal content-->
                                                                                    <div class="modal-content container-fluid">
                                                                                        <div class="modal-header">
                                                                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                                                            <h4 class="modal-title">Payments Processing</h4>
                                                                                        </div>
                                                                                        <div class="modal-body container-fluid">
                                                                                            <div class="col-sm-8 col-sm-offset-0" id="obj5733e78df37b4a276f98e9c2">
                                                                                                <div class="bool-container panel panel-primary">
                                                                                                    <div class="panel-heading" id="obj5733e78df37b4a276f98e9c1">
                                                                                                        <div class="bool-container panel-title">
                                                                                                            <h3>List Of Payers</h3></div>
                                                                                                    </div>
                                                                                                    <div class="bool-container panel-body">
                                                                                                        <div ng-repeat="item in top5users track by $index" class="bubble me col-sm-4">
                                                                                                            <p>User ID: {{item._id}}</p>
                                                                                                            <p>From: {{item.uname}}</p>
                                                                                                            <p>To: {{currentPayee.uname}}</p>
                                                                                                            <p>Message: Please deposit E {{currentPayee.amount}}.00 to: {{currentPayee.fname +' '+ currentPayee.lname}} Acc: {{currentPayee.acc}} | {{currentPayee.accholder}} contact number: {{currentPayee.contact}}. You have 24 hours to complete this request. Thank you! </p>
                                                                                                            <p>Amount: E {{currentPayee.amount}}.</p>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="col-sm-4 col-sm-offset-0" id="obj5733e78df37b4a276f98e9c221">
                                                                                                <div class="bool-container panel panel-primary">
                                                                                                    <div class="panel-heading" id="obj5733e78df37b4a276f98e9c11">
                                                                                                        <div class="bool-container panel-title">
                                                                                                            <h3>List of Payees</h3></div>
                                                                                                    </div>
                                                                                                    <div class="bool-container panel-body">
                                                                                                        <div class="bubble me col-sm-10">
                                                                                                            <p>Proxy ID: {{currentPayee._id}}</p>
                                                                                                            <p>Username: {{currentPayee.uname}}</p>
                                                                                                            <p>Full Name: {{currentPayee.fname +' '+ currentPayee.lname}}</p>
                                                                                                            <p>Prev donation: E {{currentPayee.amount}}</p>
                                                                                                            <p>Level: {{currentPayee.level}}</p>
                                                                                                            <p>Day: 0 of every month</p>
                                                                                                            <p>Expected Pay: E {{currentPayee.amount * 5}}</p>
                                                                                                            <p>
                                                                                                                <button type="button" class="btn btn-info btn-sm" ng-click="runInitPayOut(currentPayee._id)">Run Payout</button>
                                                                                                            </p>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="modal-footer">
                                                                                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="tab-pane fade bool-container" id="level4"> </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>
